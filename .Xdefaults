URxvt.saveLines:	1000

! Scrollbar
URxvt.scrollStyle:	plain
URxvt.scrollBar_right:	true
URxvt.scrollBar:	false

! Font
URxvt.allow_bold:	true
!URxvt.font: 		xft:Monospace:size=11:antialias=true
!URxvt*font:		xft:Inconsolata\ for\ Powerline:bold:size=12:antialias=true
URxvt*font:		xft:SauceCodePro\ Nerd\ Font\ Mono:bold:size=12:antialias=true,\
			xft:IPAGothic:size=11:antialias=true
URxvt.letterSpace:	0

! Bell
URxvt.urgentOnBell:	true
URxvt.visualBell:	true

! Perl
URxvt.perl-ext-common: 	xim-onthespot,tabbed

! Tab
URxvt.tabbed.tabbar-fg: 15
URxvt.tabbed.tabbar-bg: 10
URxvt.tabbed.tab-fg: 	10
URxvt.tabbed.tab-bg: 	15

! Input method
URxvt.inputMethod: 	fcitx
URxvt.preeditType: 	OnTheSpot

! Cursor
URxvt.cursorBlink:	true
URxvt.cursorUnderline:	false

